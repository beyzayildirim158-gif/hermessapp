      "eas": {
        "projectId": "373a40d7-3654-4794-9dbf-14e97bfc7c8e"
      }
    },
    "owner": "hermesshealing",
    "plugins": [
      "expo-font"
    ],
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/373a40d7-3654-4794-9dbf-14e97bfc7c8e"
